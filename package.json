{
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js/issues"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.9.3",
    "@xyo-network/react-app-settings": "workspace:^",
    "@xyo-network/react-appbar": "workspace:^",
    "@xyo-network/react-archive": "workspace:^",
    "@xyo-network/react-archivist": "workspace:^",
    "@xyo-network/react-archivist-api": "workspace:^",
    "@xyo-network/react-auth": "workspace:^",
    "@xyo-network/react-auth-service": "workspace:^",
    "@xyo-network/react-block": "workspace:^",
    "@xyo-network/react-card": "workspace:^",
    "@xyo-network/react-footer": "workspace:^",
    "@xyo-network/react-login-forms": "workspace:^",
    "@xyo-network/react-map": "workspace:^",
    "@xyo-network/react-network": "workspace:^",
    "@xyo-network/react-panel": "workspace:^",
    "@xyo-network/react-payload": "workspace:^",
    "@xyo-network/react-payload-plugin": "workspace:^",
    "@xyo-network/react-property": "workspace:^",
    "@xyo-network/react-schema": "workspace:^",
    "@xyo-network/react-shared": "workspace:^",
    "@xyo-network/react-theme": "workspace:^",
    "@xyo-network/react-typedoc": "workspace:^",
    "@xyo-network/react-wallet": "workspace:^",
    "@xyo-network/react-wallet-service": "workspace:^",
    "@xyo-network/react-webapp": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "description": "Common React library for all XYO projects that use React",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-viewport": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/api": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/components": "^6.5.9",
    "@storybook/core-events": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.9",
    "@xylabs/eslint-config-react": "^2.1.7",
    "@xylabs/react-flexbox": "^2.14.10",
    "@xylabs/react-invertable-theme": "^2.14.10",
    "@xylabs/ts-scripts": "^2.5.13",
    "@xylabs/tsconfig-dom": "^2.5.13",
    "assert": "^2.0.0",
    "chromatic": "^6.7.3",
    "cpy-cli": "^4.1.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.21.0",
    "rimraf": "^3.0.2",
    "storybook-dark-mode": "^1.1.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "react"
  ],
  "license": "LGPL-3.0",
  "name": "@xyo-network/sdk-xyo-react",
  "resolutions": {
    "bn.js": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js.git"
  },
  "scripts": {
    "build": "echo Building... && yarn compile && yarn gen:docs && yarn lint-fast",
    "compile": "echo Compiling... && yarn compile:esm && yarn compile:cjs && yarn copy-images",
    "compile:cjs": "echo Compiling CJS... && yarn workspaces foreach -pAt run compile:package:cjs",
    "compile:esm": "echo Compiling ESM... && yarn workspaces foreach -pAt run compile:package:esm",
    "compile:stories": "echo Building Stories... && tsc -p $INIT_CWD/tsconfig.build.stories.json",
    "copy-images": "echo Copying Images... && yarn copy-images:esm && yarn copy-images:cjs",
    "copy-images:cjs": "yarn workspaces foreach -ptR run copy-images:package:cjs",
    "copy-images:esm": "yarn workspaces foreach -ptR run copy-images:package:esm",
    "gen:docs": "echo Generating Docs... && yarn workspaces foreach -p run gen:docs:package",
    "build-storybook": "build-storybook ./src",
    "lint-pkg": "npmPkgJsonLint .",
    "deploy3": "yarn deploy-patch",
    "deploy-major": "yarn workspaces foreach --all version major --deferred && yarn deploy-common",
    "deploy-minor": "yarn workspaces foreach --all version minor --deferred && yarn deploy-common",
    "deploy-patch": "yarn workspaces foreach --all version patch --deferred && yarn deploy-common",
    "deploy-common": "echo Deploy... && yarn clean && yarn build && yarn apply-version:private && yarn publish:private",
    "apply-version:private": "echo Applying Version... && yarn version apply --all",
    "publish:private": "echo Publishing... && yarn workspaces foreach -pt npm publish",
    "clean": "echo Cleaning... && yarn workspaces foreach -Ap run clean:package",
    "deps": "yarn workspaces foreach -Apv run deps:package",
    "lint": "echo Linting... && yarn eslint .",
    "lint-fast": "echo Linting... && yarn eslint . --cache",
    "cycle": "yarn workspaces foreach -Apv run cycle:package",
    "fix": "echo Fixing... && yarn eslint . --fix",
    "fix-fast": "echo Fixing... && yarn eslint . --fix --cache",
    "outdated": "yarn workspaces foreach -Apv outdated",
    "up": "yarn workspaces foreach -pv up",
    "updo": "yarn workspaces foreach -pv run updo3:package",
    "gen:docs:package": "typedoc --logLevel Error --tsconfig $INIT_CWD/tsconfig.json --excludeExternals $INIT_CWD/src/index.ts --json $INIT_CWD/dist/docs.json",
    "clean:package": "rimraf -q $INIT_CWD/dist && rimraf -q $INIT_CWD/build",
    "fix:package": "yarn eslint $INIT_CWD/src --fix",
    "cycle:package": "yarn eslint --rule \"import/no-cycle:[1, { maxDepth: 10 }]\" $INIT_CWD/src",
    "deps:package": "cd $INIT_CWD && depcheck .",
    "updo3:package": "yarn update-interactive",
    "compile:package:cjs": "tsc -p $INIT_CWD/tsconfig.build.cjs.json",
    "compile:package:esm": "tsc -p $INIT_CWD/tsconfig.build.esm.json",
    "copy-images:package:esm": "cpy \"$INIT_CWD/src/**/*.svg\" \"$INIT_CWD/src/**/*.jpg\" \"$INIT_CWD/src/**/*.gif\" \"$INIT_CWD/src/**/*.png\" $INIT_CWD/dist/esm",
    "copy-images:package:cjs": "cpy \"$INIT_CWD/src/**/*.svg\" \"$INIT_CWD/src/**/*.jpg\" \"$INIT_CWD/src/**/*.gif\" \"$INIT_CWD/src/**/*.png\" $INIT_CWD/dist/cjs",
    "start": "start-storybook -p 6006"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "version": "2.27.0",
  "packageManager": "yarn@3.2.1"
}
