{
  "name": "@xyo-network/sdk-xyo-react-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Common React library for all XYO projects that use React",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "react"
  ],
  "license": "LGPL-3.0",
  "private": false,
  "version": "2.0.66",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -r -f dist && yarn deps && yarn lint && yarn compile && yarn copy-src",
    "copy-src": "mkdir -p dist && cp package.json dist && cp README.md dist && cp LICENSE dist && cp -R src/* dist",
    "compile": "tsc && rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn build && yarn publish ./dist",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint-pkg": "npmPkgJsonLint .",
    "fix": "eslint --ext .ts,.tsx ./src --fix",
    "test": "env TS_NODE_PROJECT=\"tsconfig.test.json\" mocha -r ts-node/register 'src/**/*.spec.ts'",
    "clean": "rm -r -f dist && rm -f .eslintcache && rm -f yarn.lock && rm -f *.tsbuildinfo",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune",
    "deps": "yarn depcheck"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "numeral": "^2.0.6",
    "query-string": "^7.0.0",
    "react-cookie-consent": "^6.2.4",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0"
  },
  "peerDependencies": {
    "@material-ui/core": ">=5.0.0-alpha.37",
    "@material-ui/styles": ">=5.0.0-alpha.35",
    "@xylabs/pixel": "^1.0.15",
    "@xyo-network/sdk-xyo-js": "^2.1.22",
    "firebase": ">=8",
    "react": ">=17",
    "react-dom": ">=17",
    "react-router-dom": ">=5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-alpha.37",
    "@material-ui/styles": "^5.0.0-alpha.35",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^15.12.2",
    "@types/numeral": "^2.0.1",
    "@types/react": "^17.0.9",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@xylabs/pixel": "^1.0.15",
    "@xyo-network/sdk-xyo-js": "^2.1.22",
    "depcheck": "^1.4.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "jest": "^27.0.4",
    "npm-package-json-lint": "^5.1.0",
    "npm-package-json-lint-config-default": "^3.0.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rollbar": "^2.21.1",
    "rollup": "^2.52.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.3",
    "vue": "^3.1.1"
  }
}