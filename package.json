  ],
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.0",
    "@xylabs/sdk-react": "^2.12.6",
    "@xyo-network/api": "^2.20.33",
    "@xyo-network/core": "^2.20.33",
    "@xyo-network/react-appbar": "workspace:^",
    "@xyo-network/react-archive": "workspace:^",
    "@xyo-network/react-archivist-api": "workspace:^",
    "@xyo-network/react-auth": "workspace:^",
    "@xyo-network/react-auth-service": "workspace:^",
    "@xyo-network/react-block": "workspace:^",
    "@xyo-network/react-login-forms": "workspace:^",
    "@xyo-network/react-network": "workspace:^",
    "@xyo-network/react-panel": "workspace:^",
    "@xyo-network/react-payload": "workspace:^",
    "@xyo-network/react-property": "workspace:^",
    "@xyo-network/react-schema": "workspace:^",
    "@xyo-network/react-shared": "workspace:^",
    "@xyo-network/react-theme": "workspace:^",
    "@xyo-network/react-wallet": "workspace:^",
    "@xyo-network/react-wallet-service": "workspace:^",
    "@xyo-network/utils": "^2.20.33",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "typedoc": "^0.22.15"
  },
  "description": "Common React library for all XYO projects that use React",
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@storybook/addon-actions": "^6.5.4",
    "@storybook/addon-docs": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-interactions": "^6.5.4",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/addons": "^6.5.4",
    "@storybook/api": "^6.5.4",
    "@storybook/builder-webpack5": "^6.5.4",
    "@storybook/components": "^6.5.4",
    "@storybook/core-events": "^6.5.4",
    "@storybook/manager-webpack5": "^6.5.4",
    "@storybook/react": "^6.5.4",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/theming": "^6.5.4",
    "@xylabs/eslint-config-react": "^2.1.5",
    "@xylabs/sdk-react": "^2.12.6",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "axios": "^0.27.2",
    "storybook-dark-mode": "^1.1.0",
    "typescript": "^4.6.4"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "react"
  ],
  "license": "LGPL-3.0",
  "name": "@xyo-network/sdk-xyo-react",
  "peerDependencies": {
    "axios": ">0.26",
    "typedoc": "^0.22.13"
  },
  "peerDependenciesMeta": {
    "typedoc": {
      "optional": true
    }
  },
  "resolutions": {
    "@storybook/react/webpack": "^5",
    "bn.js": "^5.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "webpack": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js.git"
  },
  "scripts": {
    "build-all": "yarn workspaces foreach -ptR run build",
    "build-storybook": "build-storybook ./src",
    "lint-pkg": "npmPkgJsonLint .",
    "deploy3": "yarn deploy-patch",
    "deploy-major": "yarn workspaces foreach version major --deferred && yarn deploy-common",
    "deploy-minor": "yarn workspaces foreach version minor --deferred && yarn deploy-common",
    "deploy-patch": "yarn workspaces foreach version patch --deferred && yarn deploy-common",
    "deploy-common": "yarn cycle && yarn workspaces foreach run build && yarn version apply --all && yarn workspaces foreach npm publish",
    "clean-all": "yarn workspaces foreach run clean",
    "deps-all": "yarn workspaces foreach run deps",
    "fix": "yarn workspaces foreach run fix",
    "start": "start-storybook -p 6006",
    "build": "yarn clean && tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json && yarn gen:docs",
    "gen:docs": "typedoc ./src/index.ts --json ./dist/docs.json --tsconfig ./tsconfig.json"
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "version": "2.23.6",
  "packageManager": "yarn@3.1.1"
}
