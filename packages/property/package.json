{
  "name": "@xyo-network/react-property",
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.1",
    "@xylabs/sdk-react": "^2.12.8",
    "@xyo-network/core": "^2.20.33",
    "@xyo-network/react-shared": "workspace:^",
    "@xyo-network/react-theme": "workspace:^",
    "lodash": "^4.17.21",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15"
  },
  "description": "Common React library for all XYO projects that use React",
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@storybook/addons": "^6.5.4",
    "@storybook/api": "^6.5.4",
    "@storybook/components": "^6.5.4",
    "@storybook/core-events": "^6.5.4",
    "@storybook/react": "^6.5.4",
    "@storybook/theming": "^6.5.4",
    "@types/lodash": "^4.14.182",
    "@xylabs/sdk-react": "^2.12.8",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "axios": "^0.27.2",
    "storybook-dark-mode": "^1.1.0",
    "typescript": "^4.6.4"
  },
  "browser": "dist/esm/index.js",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "homepage": "https://xyo.network",
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "react"
  ],
  "license": "LGPL-3.0",
  "peerDependencies": {
    "axios": ">0.26",
    "typedoc": "^0.22.13"
  },
  "peerDependenciesMeta": {
    "typedoc": {
      "optional": true
    }
  },
  "resolutions": {
    "@storybook/react/webpack": "^5",
    "bn.js": "^5.2.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "webpack": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js.git"
  },
  "scripts": {
    "deploy": "yarn deploy3",
    "lint-pkg": "npmPkgJsonLint .",
    "build": "yarn clean && tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json && yarn gen:docs",
    "gen:docs": "typedoc ./src/index.ts --json ./dist/docs.json --tsconfig ./tsconfig.json",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\""
  },
  "sideEffects": false,
  "types": "dist/esm/index.d.ts",
  "version": "2.23.7",
  "packageManager": "yarn@3.1.1"
}
